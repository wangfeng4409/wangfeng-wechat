<!--pages/home/childCpns/w-recommend/w-recommend.wxml-->
<view class="recommend">
  <block
    wx:for="{{recommends}}"
    wx:key="index"
  >
    <view class="recommend-item">
      <image class="recommend-item-image" src="{{item.image}}" />
      <view>
        <text class="recommend-item-text">{{item.title}}</text>
      </view>
    </view>
  </block>
</view>
